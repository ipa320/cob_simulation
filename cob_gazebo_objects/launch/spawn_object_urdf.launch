<?xml version="1.0"?>
<launch>
	<arg name="object"/>
	<arg name="file_path_urdf" default="$(find cob_gazebo_objects)/objects/$(arg object).urdf"/>
	<arg name="object_origin" default=""/>
	<arg name="x" default="0.0"/>
	<arg name="y" default="0.0"/>
	<arg name="z" default="0.0"/>


	<param name="object_description" textfile="$(arg file_path_urdf)" />

	<node name="spawn_object_urdf" pkg="gazebo_ros" type="spawn_model" args="-urdf -param object_description -model $(arg object) -x $(arg x) -y $(arg y) -z $(arg z)" respawn="false" output="screen" />

</launch>
