  <buildtool_depend>catkin</buildtool_depend>
  <run_depend>cob_gazebo</run_depend>
  <run_depend>cob_bringup_sim</run_depend>
  <run_depend>cob_gazebo_objects</run_depend>
  <run_depend>cob_gazebo_worlds</run_depend>





  <export>
    <metapackage/>
  </export>
