<?xml version="1.0"?>
<launch>

	<!-- send urdf to param server -->
	<param name="coffee_cup" textfile="$(find gazebo_worlds)/objects/coffee_cup.model" />

	<!-- push urdf to factory and spawn robot in gazebo -->
	<node name="spawn_coffee_cup" pkg="gazebo" type="spawn_model" args="-gazebo -param coffee_cup -x -3.0 -y -0.0 -z .98  -R 0 -P 0 -Y 1.5708 -model coffee_cup" respawn="false" output="screen" />

</launch>
