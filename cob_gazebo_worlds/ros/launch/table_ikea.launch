<?xml version="1.0"?>
<launch>

	<!-- send table urdf to param server -->
	<param name="table_description" command="$(find xacro)/xacro.py $(find cob_gazebo_worlds)/objects/table_ikea.urdf.xacro" />

	<!-- push table_description to factory and spawn robot in gazebo -->
	<node name="spawn_table" pkg="gazebo" type="spawn_model" args="-urdf -param table_description -x 1.1 -y 1.2 -z 0.1 -model table_model" respawn="false" output="screen" />

</launch>
